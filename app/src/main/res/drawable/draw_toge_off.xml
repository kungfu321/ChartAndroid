<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="5dp" android:bottom="5dp">
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="10dp" android:topRightRadius="10dp" android:bottomRightRadius="10dp" android:bottomLeftRadius="10dp"/>
            <solid android:color="#bebebe"/>
        </shape>
    </item>

    <item android:right="20dp">
        <shape android:shape="oval">
            <solid android:color="#29ced4"/>
            <size android:height="30dp" android:width="30dp"/>
        </shape>
    </item>

</layer-list>